{
  "name": "code-snippets",
  "displayName": "code-snippets",
  "description": "code-snippets",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other",
    "Snippets"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "code-snippets.create",
        "title": "login database"
      },
      {
        "command": "code-snippets.insert",
        "title": "inject snippets"
      },
      {
        "command": "code-snippets.edit",
        "title": "edit snippets "
      },
      {
        "command": "code-snippets.search",
        "title": "search  snippets"
      },
      {
        "command": "code-snippets.sync",
        "title": "sync snippets"
      },
      {
        "command": "code-snippets.update",
        "title": "update snippets"
      },
      {
        "command": "code-snippets.delete",
        "title": "delete snippets"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "code-snippets.insert",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "code-snippets.search",
          "group": "search"
        }
      ],
      "explorer/context": [
        {
          "command": "code-snippets.search",
          "group": "navigation"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "code-snippets",
          "title": "code-snippets",
          "icon": "media/logo.svg"
        }
      ]
    },
    "views": {
      "code-snippets": [
        {
          "id": "code-snippets.web",
          "name": "snippets web",
          "contextualTitle": "Code snippets",
          "type": "webview"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.81.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@types/uuid": "^9.0.2",
    "code-snippets-sdk-node": "0.0.8-alpla",
    "isomorphic-fetch": "^3.0.0",
    "uuid": "^9.0.0"
  }
}
